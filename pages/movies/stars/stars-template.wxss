.stars-container {
  display: flex;
  flex-direction: row;
  margin-top: 10rpx;
  align-items: center;
}
.stars {
  flex: 5;
  display: flex;
}
.stars>image {
  flex: 1;
  width: 20rpx;
  height: 30rpx;
  margin-right: 5rpx;
}
.star-score {
  flex: 1;
  margin-left: 10rpx;
  font-size: 24rpx;
}
